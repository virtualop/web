      <h1>dev</h1>
    </div>
    <div id="buttons">
      <a class="btn btn-primary" href="/dev/refresh" data-turbolinks="false" role="button">refresh</a>
    </div>
  </div>

  <% @working_copies.each do |working_copy| %>
